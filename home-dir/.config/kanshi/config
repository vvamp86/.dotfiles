profile DP-1-main {
  output eDP-1 mode 2256x1504 position 0,0
  output DP-1 mode 1920x1200 position 2256,0
  exec swaymsg workspace 1, move workspace to output 'eDP-1'
  exec swaymsg workspace 3, move workspace to output 'DP-1'
  exec swaymsg workspace 4, move workspace to output 'DP-1'
  exec swaymsg workspace 5, move workspace to output 'DP-1'
  exec swaymsg workspace 6, move workspace to output 'DP-1'
  exec swaymsg workspace 7, move workspace to output 'DP-1'
  exec swaymsg workspace 8, move workspace to output 'DP-1'
  exec swaymsg workspace 9, move workspace to output 'DP-1'
  exec swaymsg workspace 2, move workspace to output 'DP-1'
}

